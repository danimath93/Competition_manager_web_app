  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Backend",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/server/index.js",
      "cwd": "${workspaceFolder}/server"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Frontend",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "start"
      ],
      "cwd": "${workspaceFolder}/client",
      "console": "integratedTerminal"
    }
  ]}